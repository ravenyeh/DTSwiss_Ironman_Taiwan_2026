{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "modules/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/garmin/login",
      "dest": "/api/garmin/login.js"
    },
    {
      "src": "/api/garmin/token-login",
      "dest": "/api/garmin/token-login.js"
    },
    {
      "src": "/api/garmin/logout",
      "dest": "/api/garmin/logout.js"
    },
    {
      "src": "/api/garmin/workout",
      "dest": "/api/garmin/workout.js"
    },
    {
      "src": "/api/garmin/import",
      "dest": "/api/garmin/import.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
